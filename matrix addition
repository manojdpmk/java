import java.util.Scanner;
public class ADDMATRICS{
 public static void main(String[] args){
 int[][] a={{1,2},{5,3}};
 int[][] b={{2,3},{4,1}};
 int[][] c=new int[2][2];
 for(int i=0;i<3;i++){
 for(int j=0;j<3;j++){
 c[i][j]=a[i][j]+b[i][j];
 System.out.print(c[i][j]+" ");
 }
 System.out.println();
 }
 }
}
